{
  "name": "m-nav",
  "version": "0.5.2",
  "private": true,
  "keywords": [
    "m-nav",
    "notion",
    "navigation",
    "nextjs",
    "react",
    "typescript"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "docker:build": "docker build -t m-nav .",
    "docker:dev": "docker compose up -d",
    "docker:stop": "docker compose down",
    "docker:push": "docker tag m-nav busyhe/m-nav:latest && docker tag m-nav busyhe/m-nav:$(node -p \"require('./package.json').version\") && docker push busyhe/m-nav:latest && docker push busyhe/m-nav:$(node -p \"require('./package.json').version\")",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "LEFTHOOK=0 bumpp package.json apps/*/package.json --all --execute \"pnpm docker:build && pnpm docker:push\""
  },
  "devDependencies": {
    "@m-nav/eslint-config": "workspace:*",
    "@m-nav/typescript-config": "workspace:*",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "lefthook": "catalog:",
    "prettier": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook",
      "core-js-pure",
      "sharp"
    ]
  }
}